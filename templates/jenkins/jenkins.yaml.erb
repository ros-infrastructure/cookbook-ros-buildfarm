    - "method org.w3c.dom.Node appendChild org.w3c.dom.Node"
    - "method org.w3c.dom.Node setTextContent java.lang.String"
    - "method org.w3c.dom.NodeList getLength"
    - "method org.w3c.dom.NodeList item int"
